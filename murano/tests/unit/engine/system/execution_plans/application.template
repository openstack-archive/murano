FormatVersion: 2.0.0
Version: 1.0.0
Name: Deploy Tomcat

Parameters:
  appName: $appName

Body: |
  return deploy(args.appName).stdout

Scripts:
  deploy:
    Type: Application
    Version: 1.0.0
    EntryPoint: deployTomcat.sh
    Files:
      - installer: <installer.sh>
      - <common.sh>
    Options:
      captureStdout: true
      captureStderr: true
